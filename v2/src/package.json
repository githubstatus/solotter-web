{
  "name": "solotter-web",
  "version": "1.2.0",
  "description": "Web app for twitter solo-players",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node ../dist/index.js",
    "dev": "npm run watch",
    "prod": "run-s test build",
    "watch": "run-p watch:s:* watch:c:*",
    "watch:s:typescript": "cross-env NODE_ENV=development tsc -w -p ./tsconfig-server.json",
    "watch:s:test": "chokidar --initial ../dist/{core,server}/**.spec.js -c 'cross-env NODE_ENV=development mocha --reporter list ../dist/{core,server}/**.spec.js'",
    "watch:c": "run-p watch:c:*",
    "watch:c:webpack": "cross-env NODE_ENV=development webpack --watch",
    "watch:c:server": "cross-env NODE_ENV=development webpack-dev-server --watch",
    "watch:c:test": "cross-env NODE_ENV=development karma start",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "run-s test:build test:karma",
    "test:build": "cross-env NODE_ENV=development webpack",
    "test:karma": "cross-env NODE_ENV=development karma start --single-run"
  },
  "engines": {
    "node": "8.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vain0/Solotter-on-Web.git"
  },
  "author": "vain0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vain0/Solotter-on-Web/issues"
  },
  "homepage": "https://github.com/vain0/Solotter-on-Web#readme",
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.34",
    "@types/csurf": "^1.9.35",
    "@types/express": "^4.11.1",
    "@types/mocha": "^2.2.48",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.7",
    "@types/serve-static": "^1.13.1",
    "assert-diff": "^1.2.5",
    "chai": "^4.1.2",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "dts-gen": "^0.5.7",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-eslint-plugin": "^1.4.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "ifdef-loader": "^2.0.3",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^3.0.12",
    "karma-webpack": "^2.0.13",
    "mocha": "^5.0.4",
    "mocha-reporter": "^0.1.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
