{
    "extends": [],
    "defaultSeverity": "warn",
    "rules": {
        "semicolon": [
            true,
            "never",
            "ignore-interfaces"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "max-classes-per-file": false,
        "indent": true,
        "space-before-function-paren": false,
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "ordered-imports": true
    },
    "rulesDirectory": []
}